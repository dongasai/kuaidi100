{
    "name": "dongasai/kuaidi100",
    "description": "基于快递100，查询智能推送，云打印，短信",
    "license": "MIT",
    "authors": [
        {
            "name": "harriescc",
            "email": "harrieschen@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": ">=6.3",
        "phpunit/phpunit": ">=6",
        "ext-json": "*",
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "HarriesCC\\Kuaidi100\\": "src"
        }
